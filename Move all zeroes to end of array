class Solution {
    void pushZerosToEnd(int[] arr, int n) {
        // code here
        int pos=0;
        for(int i=0;i<arr.length;i++)
        {
            if(arr[i]!=0)
            {
                arr[pos]=arr[i];
                pos++;
            }
        }
        while(pos<n)
        {
            arr[pos]=0;
            pos++;
        }
    }
}
